
import UIKit

class BCSCreatedBy: NSObject {
    
    var id: Int?
    var type: Int?
    var name: String?
    var thumbnail: String?
    
    override init() {
        super.init()
    }
    
    init(userDictionary: NSDictionary) {
        super.init()
        
       id = userDictionary.value(forKeyPath: "createdby.id") as? Int
       type = userDictionary.value(forKeyPath: "createdby.type") as? Int
       name = userDictionary.value(forKeyPath: "createdby.name") as? String
       thumbnail = userDictionary.value(forKeyPath: "createdby.thumbnail") as? String
        
    }
    
}
